      <DiagnosticReport xmlns="http://hl7.org/fhir">
        <id value="{{ report_uuid }}" />
        <meta>
          <profile value="https://fhir.hl7.org.uk/STU3/StructureDefinition/CareConnect-DiagnosticReport-1" />
        </meta>
        <!--Test Request Summary-->
        <basedOn>
          <reference value="urn:uuid:{{ basedon_uuid }}" />
        </basedOn>
        <status value="{{ status }}" />
        <code>
          <coding>
            <system value="http://snomed.info/sct" />
            <code value="721981007" />
            <display value="Diagnostic studies report" />
          </coding>
        </code>
        <subject>
          <reference value="urn:uuid:{{ patient_uuid }}" />
          <display value="{{ patient_display }}" />
        </subject>
        <issued value="{{ issuedon }}" />
        <performer>
          <actor>
            <reference value="urn:uuid:{{ performer_uuid }}" />
            <display value="{{ performer_display }}" />
          </actor>
        </performer>
        {% for specimen_uuid in specimen_uuids %}
        <specimen>
          <reference value="urn:uuid:{{ specimen_uuid }}" />
        </specimen>
        {% endfor %}
        {% for result_uuid in result_uuids %}
        <result>
          <reference value="urn:uuid:{{ result_uuid }}" />
        </result>
        {% endfor %}
      </DiagnosticReport>