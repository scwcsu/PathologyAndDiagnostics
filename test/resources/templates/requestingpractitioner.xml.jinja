      <Practitioner>
        <id value="{{ practitioner_uuid }}" />
        <meta>
          <profile value="https://fhir.hl7.org.uk/STU3/StructureDefinition/CareConnect-Practitioner-1" />
        </meta>
        <!--where to store national code for GP ??-->
        <identifier>
          <system value="https://fhir.nhs.uk/Id/sds-user-id" />
          <value value="{{ practitioner_id }}" />
        </identifier>
        <name>
          <family value="{{ family_name }}" />
          <given value="{{ given_name }}" />
        </name>
      </Practitioner>
