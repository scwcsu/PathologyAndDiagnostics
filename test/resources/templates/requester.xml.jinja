<Organization>
    <id value="{{ requester_uuid }}" />
    <meta>
      <profile value="https://fhir.hl7.org.uk/STU3/StructureDefinition/CareConnect-Organization-1" />
    </meta>
    <identifier>
      <system value="https://fhir.nhs.uk/Id/ods-organization-code" />
      <value value="{{ requester_identifier }}" />
    </identifier>
    <name value="{{ requester_name }}" />
    <address>
      <line value="{{ requester_address_line }}" />
      <city value="{{ requester_city }}" />
      <district value="{{ requester_district }}" />
      <postalCode value="{{requester_postcode }}" />
    </address>
  </Organization>
